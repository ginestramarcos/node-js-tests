{
  "name": "node-js-tests",
  "version": "0.0.0",
  "description": "Me learning node, typescript, ecmascript 6, etc.",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./dist/server/main.js",
    "build": "rm -Rf dist/ && npm run tsc && rm -Rf dist/client && webpack --mode production",
    "test": "npx jest",
    "tsc": "tsc",
    "dev": "rm -Rf dist/ && webpack --mode development --config webpack-dev.config.js & ts-node-dev --respawn --transpile-only src/server/main.ts",
    "lint": "eslint .",
    "submit": "npm run checkDoNotSubmit && npm run lint && npm run build && npm test && git push",
    "checkDoNotSubmit": "! grep -R -i --exclude-dir={node_modules,.git} --exclude=package.json 'DO.NOT.SUBMIT' ."
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.1",
    "@types/nunjucks": "^3.1.3",
    "autobind-decorator": "^2.4.0",
    "cookie-parser": "~1.4.5",
    "express": "~4.17.3",
    "http-errors": "~1.8.0",
    "morgan": "~1.10.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ginestramarcos/node-js-tests.git"
  },
  "author": "Marcos Ginestra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ginestramarcos/node-js-tests/issues"
  },
  "homepage": "https://github.com/ginestramarcos/node-js-tests#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "install": "^0.13.0",
    "jest": "^26.4.2",
    "node-dev": "^6.7.0",
    "npm": "^9.3.0",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.12",
    "ts-node-dev": "^1.0.0-pre.63",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.3.0"
  }
}
